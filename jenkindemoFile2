pipeline{
agent any
environment {
    myfirstVar = "test"
}

stages{
   stage('Build'){
      steps{
      //def myfirstVar = "akash"
       sh 'mvn clean package'
       }
       post
        {
          success
          {
            echo "now archieving"
            sh "echo $myfirstVar"
            archiveArtifacts artifacts: '**/*.war'
          }
         }
              }
   }
   }
